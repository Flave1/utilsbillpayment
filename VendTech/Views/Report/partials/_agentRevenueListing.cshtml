@model PagingResult<AgentRevenueListingModel>


@if (Model.List != null && Model.List.Count() > 0)
{
    foreach (var item in Model.List)
    {
        <tr class="odd gradeX">
            <td style="text-align:right"> @item.CreatedAt</td>
            <td style="text-align:right"> @item.PosNumber</td>
            <td style="text-align:left">@item.VendorName</td>
            <td> @item.Type</td>
            <td style="text-align:center;"> @item.TransactionId</td>
            <td style="text-align:right"> @item.ChkNoOrSlipId</td>
            <td style="text-align:right"><strong><a href="#">  @VendTech.BLL.Common.Utilities.FormatAmount(item.Amount)</a></strong></td>
            <td style="text-align:right"><strong>  @VendTech.BLL.Common.Utilities.FormatAmount(item.VendorPercentageAmount)</strong></td>
            <td style="text-align:right"><strong>  @VendTech.BLL.Common.Utilities.FormatAmount(item.AgentPercentageAmount)</strong></td>

        </tr>
    }
}
else
{
    <tr class="odd gradeX">
        <td colspan="10" style="font-weight:bold;">SEARCH COMPLETE! No marching record found</td>
    </tr>
}


