@model List<SelectListItem>
<select id="rpt" onchange="changeReport(this.value)" class="form-control">
    @if (Model.Count() > 0 && Model.Any(e => e.Text == "SALES REPORT"))
    {
        <option value="2" selected="selected">SALES REPORT</option>
    }
    @if (Model.Count() > 0 && Model.Any(e => e.Text == "DEPOSITS REPORT"))
    {
        <option value="1">DEPOSIT REPORT</option>
    }
    @if (Model.Count() > 0 && Model.Any(e => e.Text == "BALANCE SHEET REPORT"))
    {
        <option value="3">BALANCE SHEET REPORT</option>
    }

    @if (Model.Count() > 0 && Model.Any(e => e.Text == "GST SALES REPORT"))
    {
        <option value="4">GST SALES REPORT</option>
    }
    @if (Model.Count() > 0 && Model.Any(e => e.Text == "AGENTS REVENUE REPORT"))
    {
        <option value="5">AGENCY REVENUE REPORT</option>
    }
</select>
