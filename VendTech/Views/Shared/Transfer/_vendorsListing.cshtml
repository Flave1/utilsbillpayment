@model PagingResult<AgentListingModel>

@{
    Layout = null;
}


<div class="transferFromOverlay" id="transferFromModal" >
    <div class="row transferFromModal">
        <div class="col-md-12">
            <div class="block-flat front">
                <div class="table-responsive">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th colspan="5"><strong>SAVED METERS</strong></th>
                            </tr>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">VENDOR NAME</th>
                                <th scope="col">POS ID</th>
                                <th scope="col">BALANCE</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.List != null)
                            {
                                int i = 0;
                                foreach (var item in Model.List)
                                {
                                    i += 1;
                                    <tr id="meterList">
                                        <th scope="row">@i</th>
                                        <td>   @item.Vendor  </td>
                                        <td>@item.POSID</td>
                                        <td>@item.Balance</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <div class="modal-body" id="fromVendorsList">
    </div>
</div>


