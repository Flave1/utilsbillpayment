@model PagingResult<DepositListingModel>


@if (Model.List != null && Model.List.Count() > 0)
{
    foreach (var item in Model.List)
    {

        <tr class="odd gradeX">
            <td style="text-align:right;">@item.CreatedAt.ToString()</td>
            <td style="text-align:right;">@item.ValueDate</td>
            <td style="text-align:right;">@item.PosNumber</td>
            <td>@item.VendorName</td> 
            <td>@item.Type</td>
            <td style="text-align:right;">@item.ChkNoOrSlipId</td>
            <td style="text-align:right;">@VendTech.BLL.Common.Utilities.FormatAmount(item.Amount)</td>
            <td style="text-align:right;">@VendTech.BLL.Common.Utilities.FormatAmount(item.PercentageAmount)</td>
            <td>@item.Status</td>
        </tr>
    }
}
else
{
    <tr class="odd gradeX">
        <td colspan="4">No Record Found</td>
    </tr>
}


