@model PagingResult<POSListingModel>
@{
    ViewBag.Title = "Manage POS";
    Layout = "~/Areas/Vendor/Views/Shared/_VendorLayOut.cshtml";
}

<div class="container-fluid" id="pcont">
    <div class="page-head">
        <h2>Manage POS</h2>

    </div>
    <div class="cl-mcont">
        <div class="row">
            <div class="col-md-12">
                <div class="block-flat">
                    <div class="header">
                        <h3>Manage POS</h3>
                    </div>
                    <div class="content">
                        <div class="table-responsive">
                            @Html.Partial("_SearchFilter", new PagingModel { SortBy = "CreatedAt", SortOrder = "Desc" })
                            <div id="divResult">
                                <table class="table table-bordered" id="datatable-icons">
                                    <thead>
                                        <tr>
                                            <th>SERIAL #</th>
                                            <th>VENDOR NAME</th>
                                            <th>CELL#</th>
                                            <th>POS TYPE</th>
                                            <th>ENABLED</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @Html.Partial("Partials/_posListing", Model)
                                    </tbody>
                                </table>
                            </div>

                        </div>
                        @Html.Partial("_Paging")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts{
    <script src="~/Scripts/UserDefinedScripts/paging.js"></script>
    <script type="text/javascript">
        var totalCount = "@Model.TotalCount";
    </script>
    <script src="~/Scripts/VendorScripts/pos.js"></script>



    <script type="text/javascript">

        $(document).ready(function () {
            //initialize the javascript
            App.init();
            Users.ManageUsers(totalCount);
        });
    </script>
}

