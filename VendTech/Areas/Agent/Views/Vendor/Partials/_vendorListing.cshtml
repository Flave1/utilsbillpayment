@model PagingResult<VendorListingModel>


@if (Model.List != null && Model.List.Count() > 0)
{
    foreach (var item in Model.List)
    {
        <tr class="odd gradeX">
            <td>@item.VendorName</td>
            <td> @item.AgencyName</td>
            <td class="pull-right">
                @(item.Balance == null ? "0" : string.Format("{0:N0}", item.Balance))
            </td>
            <td><a class="btn btn-primary btn-xs" href="@Url.Action("AddEditVendor", "Vendor", new { area = "Agent", id = @item.VendorId })" data-original-title="Edit" data-toggle="tooltip">Edit</a></td>

        </tr>
    }
}
else
{
    <tr class="odd gradeX">
        <td colspan="4">No Record Found</td>
    </tr>
}


